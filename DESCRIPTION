Package: fbi
Title: Finnish Biodiversity Indicators
Version: 0.11.22.9000
Authors@R: c(
    person(given = "Finnish Museum of Natural History - Luomus",
           role = "cph"),
    person(given = "William K.",
           family = "Morris",
           role = c("aut", "cre"),
           email = "willi@mmorris.email",
           comment = c(ORCID = "0000-0002-8686-4154"))
